{
  "buildCommand": "pnpm install --frozen-lockfile || npm ci || yarn install && pnpm build || npm run build || yarn build",
  "outputDirectory": "dist",
  "framework": "vite",
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/(assets/.*|favicon\\.ico|manifest\\.webmanifest|apple-touch-icon\\.png|icon-\\d+\\.png|robots\\.txt|.*\\.(?:js|css|svg|png|jpg|jpeg|webp|ico|map))",
      "dest": "/$1"
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ]
}
